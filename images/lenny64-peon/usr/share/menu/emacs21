  needs="X11"\
  section="Apps/Editors"\
  title="Emacs 21.4a (X11)"\
  command="/usr/bin/emacs21 -i"\
  icon="/usr/share/emacs/21.4/etc/gnu-32x32.xpm"\
  hints="Text"

